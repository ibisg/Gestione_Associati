 column=(type=char(60) updatewhereclause=yes name=livelli_descrizione dbname="struttura_livelli.descrizione" )
 column=(type=long updatewhereclause=yes identity=yes name=att_ist_livello dbname="att_ist.livello" )
 column=(type=char(60) updatewhereclause=yes name=vc_parent dbname="membri_gerarchica.vc_parent" )
 column=(type=char(60) updatewhereclause=yes name=struttura_livelli dbname="struttura_livelli.s1" )
 column=(type=char(60) updatewhereclause=yes name=tf dbname="TF" )
 column=(type=char(60) updatewhereclause=yes name=tm dbname="TM" )
 column=(type=char(60) updatewhereclause=yes name=ma dbname="MA" )
 column=(type=char(60) updatewhereclause=yes name=catt_ist_struttura dbname="catt_ist_struttura" )
 column=(type=char(60) updatewhereclause=yes name=catt_ist_nome_struttura dbname="catt_ist_nome_struttura" )
 retrieve="  SELECT membri_gerarchica.cod_div,   
         membri_gerarchica.codice,   
         membri_gerarchica.cognome,   
         membri_gerarchica.nome,  
		att_ist.cod_att_ist,
		att_ist.descrizione,   
         membri_gerarchica.flag_npa,   
         membri_gerarchica.flag_goh,   
         membri_gerarchica.flag_tok,   
         membri_gerarchica.flag_oma,   
         membri_gerarchica.cod_dis,   
         membri_gerarchica.flag_er ,
	    struttura.livello,
	    struttura.descrizione,
	    struttura.vc_nodo,
	    struttura_livelli.descrizione,
	    att_ist.livello,
         membri_gerarchica.vc_parent,
		s1.descrizione s1,
		(select riferimento_descr from riferimenti where codice= membri_gerarchica.codice and cod_rif =~"TF~" and predefinito= 1) TF,
			(select riferimento_descr from riferimenti where codice= membri_gerarchica.codice and cod_rif =~"TM~" and predefinito= 1) TM,    
		(select riferimento_descr from riferimenti where codice= membri_gerarchica.codice and cod_rif= ~"MA~" and predefinito= 1) MA,
		(select s2.vc_nodo
			 from struttura s2
			 where s2.livello=   att_ist.livello and s2.vc_nodo= substring( membri_gerarchica.vc_parent, 1, len(s2.vc_nodo)) 
			 ) catt_ist_struttura,
		(select s2.descrizione
		 from struttura s2
		 where s2.livello=  att_ist.livello and s2.vc_nodo= substring( membri_gerarchica.vc_parent, 1, len(s2.vc_nodo))  
		 )  catt_ist_nome_struttura
    FROM att_ist,
         membri_gerarchica,
		struttura,
		struttura_livelli,
		struttura_livelli s1
   WHERE ( membri_gerarchica.cod_att_ist_1 = att_ist.cod_att_ist ) and  
         ( membri_gerarchica.vc_parent like :vc_nodo+~".%~" ) and
		struttura.vc_nodo= :vc_nodo and
		struttura_livelli.livello= struttura.livello and
		att_ist.livello > 0 and
		membri_gerarchica.cod_dis= 'MEM' and membri_gerarchica.flag_er= 'S' and att_ist.livello >= :livello and att_ist.livello <= :livello_sup and
		s1.livello= att_ist.livello

UNION

  SELECT membri_gerarchica.cod_div,   
         membri_gerarchica.codice,   
         membri_gerarchica.cognome,   
         membri_gerarchica.nome,   
		att_ist.cod_att_ist,
		att_ist.descrizione,   
         membri_gerarchica.flag_npa,   
         membri_gerarchica.flag_goh,   
         membri_gerarchica.flag_tok,   
         membri_gerarchica.flag_oma,   
         membri_gerarchica.cod_dis,   
         membri_gerarchica.flag_er,
	    struttura.livello,
	    struttura.descrizione,
	    struttura.vc_nodo,
	    struttura_livelli.descrizione,
	    att_ist.livello,
         membri_gerarchica.vc_parent,
		s1.descrizione s1,
		(select riferimento_descr from riferimenti where codice= membri_gerarchica.codice and cod_rif =~"TF~" and predefinito= 1) TF,
			(select riferimento_descr from riferimenti where codice= membri_gerarchica.codice and cod_rif =~"TM~" and predefinito= 1) TM,    
		(select riferimento_descr from riferimenti where codice= membri_gerarchica.codice and cod_rif= ~"MA~" and predefinito= 1) MA,
		(select s2.vc_nodo
			 from struttura s2
			 where s2.livello=   att_ist.livello and s2.vc_nodo= substring( membri_gerarchica.vc_parent, 1, len(s2.vc_nodo)) 
			 ) catt_ist_struttura,
		(select s2.descrizione
		 from struttura s2
		 where s2.livello=  att_ist.livello and s2.vc_nodo= substring( membri_gerarchica.vc_parent, 1, len(s2.vc_nodo)) 
		 )  catt_ist_nome_struttura
    FROM att_ist,   
         membri_gerarchica,
	    struttura,
	    struttura_livelli,
		struttura_livelli s1
   WHERE ( membri_gerarchica.cod_att_ist_2 = att_ist.cod_att_ist ) and  
         ( membri_gerarchica.vc_parent like :vc_nodo+~".%~" ) and
		struttura.vc_nodo= :vc_nodo and
		struttura_livelli.livello= struttura.livello and
		att_ist.livello > 0 and
		membri_gerarchica.cod_dis= 'MEM' and membri_gerarchica.flag_er= 'S' and att_ist.livello >= :livello and att_ist.livello <= :livello_sup and
		s1.livello= att_ist.livello

