 column=(type=long updatewhereclause=yes identity=yes name=mese_versamento dbname="mese_versamento" values="Gennaio	1/Febbraio	2/Marzo	3/Aprile	4/Maggio	5/Giugno	6/Luglio	7/Agosto	8/Settembre	9/Ottobre	10/Novembre	11/Dicembre	12/" )
 column=(type=long updatewhereclause=yes identity=yes name=totale dbname="totale" )
 retrieve="   SELECT nuovi_versanti_per_anno.anno anno_versamento,   
         nuovi_versanti_per_anno.anno_entrata anno_entrata,   
         datepart(month,primo_versamento) mese_versamento,   
         count(*)  totale
    FROM nuovi_versanti_per_anno , membri_gerarchica
where nuovi_versanti_per_anno.anno= :anno and
			nuovi_versanti_per_anno.codice= membri_gerarchica.codice and
				membri_gerarchica.vc_parent like :vc_nodo+~"%~"
GROUP BY nuovi_versanti_per_anno.anno,   
         nuovi_versanti_per_anno.anno_entrata,   
         datepart(month,primo_versamento)   
   
" arguments=(("vc_nodo", string),("anno", number))  sort="anno_versamento D anno_entrata D mese_versamento D " )
