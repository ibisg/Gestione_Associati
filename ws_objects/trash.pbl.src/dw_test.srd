 retrieve="  SELECT partecipanti.cognome,   
         partecipanti.nome,   
         partecipanti.cod_div,   
         partecipanti.cod_att,   
         partecipanti.note,   
         partecipanti.codice_part  
    FROM partecipanti  
   WHERE (  not exists (select 1 from stanze where stanze.codice_part= partecipanti.codice_part and stanze.id= partecipanti.id) ) AND  
         ( partecipanti.id = :id )    
" arguments=(("id", decimal))  sort="cod_div A cognome A nome A " )
