select Totale from vw_esami_esami_curr v3
where v3.esito='P' and v3.AnnoCerimonia= vw_esami_esami_curr.AnnoCerimonia
)TotalePromossi

from 
vw_esami_esami_curr

group by AnnoCerimonia

order by AnnoCerimonia DESC


" )
